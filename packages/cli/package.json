{
  "name": "@ghostspeak/cli",
  "version": "0.1.0",
  "description": "Command-line interface for GhostSpeak AI Agent Commerce Protocol",
  "type": "module",
  "bin": {
    "ghostspeak": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solana",
    "blockchain",
    "ai-agents",
    "cli",
    "typescript",
    "web3"
  ],
  "author": "GhostSpeak Protocol",
  "license": "MIT",
  "homepage": "https://github.com/ghostspeak/ghostspeak#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostspeak/ghostspeak.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/ghostspeak/ghostspeak/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@clack/core": "^0.3.0",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "boxen": "^7.1.0",
    "figlet": "^1.7.0",
    "@types/figlet": "^1.5.0",
    "@ghostspeak/sdk": "^0.1.0",
    "@solana/web3.js": "^1.95.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}