{
  "name": "@ghostspeak/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for GhostSpeak AI Agent Commerce Protocol",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "generate": "tsx scripts/generate.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solana",
    "blockchain",
    "ai-agents",
    "defi",
    "typescript",
    "web3"
  ],
  "author": "GhostSpeak Protocol",
  "license": "MIT",
  "homepage": "https://github.com/ghostspeak/ghostspeak#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostspeak/ghostspeak.git",
    "directory": "packages/sdk-typescript"
  },
  "bugs": {
    "url": "https://github.com/ghostspeak/ghostspeak/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@solana/accounts": "^2.0.0",
    "@solana/addresses": "^2.0.0",
    "@solana/instructions": "^2.0.0",
    "@solana/kit": "^2.0.0",
    "@solana/programs": "^2.0.0",
    "@solana/rpc": "^2.0.0",
    "@solana/signers": "^2.0.0",
    "@solana/transactions": "^2.0.0"
  },
  "devDependencies": {
    "@codama/nodes": "^1.0.0",
    "@codama/nodes-from-anchor": "^1.0.0",
    "@codama/renderers-js": "^1.0.0",
    "@codama/visitors": "^1.0.0",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
