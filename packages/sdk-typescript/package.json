{
  "name": "@podai/sdk",
  "version": "2.0.4",
  "description": "TypeScript SDK for podAI Protocol on Solana",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=node --format=esm --outfile=index.esm.js && bun build src/index.ts --outdir=dist --target=node --format=cjs --outfile=index.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "codama": "node scripts/codama-generate.mjs",
    "generate": "npm run codama",
    "dev": "bun --watch src/index.ts",
    "test": "bun test",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@solana/addresses": "2.1.1",
    "@solana/rpc": "2.1.1",
    "@solana/rpc-types": "2.1.1",
    "@solana/signers": "2.1.1",
    "@solana/codecs": "2.1.1",
    "@solana/kit": "0.1.7",
    "@codama/renderers-js": "^1.0.0",
    "@codama/nodes-from-anchor": "^1.0.0",
    "codama": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "24.0.7",
    "typescript": "^5.0.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "bun-types": "^1.1.34"
  },
  "peerDependencies": {
    "@solana/web3.js": "^2.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "solana",
    "blockchain",
    "ai",
    "agents",
    "podai",
    "protocol",
    "typescript",
    "sdk"
  ],
  "author": "PodAI Team",
  "license": "MIT"
}
